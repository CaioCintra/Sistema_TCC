// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Professor {
  nome         String @unique
  email        String
  departamento String
  telefone     String
}
model Aluno {
  ra                String @unique @db.VarChar(7)
  nome              String
  email             String
  status            String @default("Matriculado_TCC1")
  periodo_matricula String
}

enum Status {
  Matriculado_TCC1
  Orientador_Definido
  Banca_TCC1_Agendada
  Banca_TCC1_Confirmada
  Aprovado_TCC1
  Reprovado_TCC1
}


